<Window x:Class="ThreeDbsPrOne.Windows.FillFillQueue"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ThreeDbsPrOne.Windows"
        mc:Ignorable="d"
        Title="FillFillQueue" Height="550" Width="350">
    <Grid>
        <Label Name ="TaskCommand"  Content="Перший Запит" FontSize ="26" 
         HorizontalAlignment="Center" VerticalAlignment="Top" Margin="10"/>
        <ListBox Name ="Resumes" BorderBrush="Black" Width="300" Height ="400" 
           HorizontalAlignment ="Center"/>
        <Button Name ="TaskBut" Height="35" Width="250" VerticalAlignment="Bottom" 
          Margin="15" Content="Видалити" FontSize="20" Click="TaskBut_Click"/>
    </Grid>
</Window>
